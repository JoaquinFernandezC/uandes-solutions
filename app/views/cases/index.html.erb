<p id="notice"><%= notice %></p>

<h1>Cases</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Regional pros office</th>
      <th>Description</th>
      <th>Privacy</th>
      <th>Ruc felony</th>
      <th>State</th>
      <th>Prosecutor</th>
      <th>Estimated end date</th>
      <th>Log</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cases.each do |case| %>
      <tr>
        <td><%= case.name %></td>
        <td><%= case.regional_pros_office %></td>
        <td><%= case.description %></td>
        <td><%= case.privacy %></td>
        <td><%= case.ruc_felony %></td>
        <td><%= case.state %></td>
        <td><%= case.prosecutor %></td>
        <td><%= case.estimated_end_date %></td>
        <td><%= case.log %></td>
        <td><%= link_to 'Show', case %></td>
        <td><%= link_to 'Edit', edit_case_path(case) %></td>
        <td><%= link_to 'Destroy', case, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Case', new_case_path %>
