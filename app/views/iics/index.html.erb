<p id="notice"><%= notice %></p>

<h1>Iics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>State</th>
      <th>Start date</th>
      <th>Estimated end date</th>
      <th>End date</th>
      <th>Log</th>
      <th>Privacy</th>
      <th>Multilateral</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @iics.each do |iic| %>
      <tr>
        <td><%= iic.name %></td>
        <td><%= iic.description %></td>
        <td><%= iic.state %></td>
        <td><%= iic.start_date %></td>
        <td><%= iic.estimated_end_date %></td>
        <td><%= iic.end_date %></td>
        <td><%= iic.log %></td>
        <td><%= iic.privacy %></td>
        <td><%= iic.multilateral %></td>
        <td><%= link_to 'Show', iic %></td>
        <td><%= link_to 'Edit', edit_iic_path(iic) %></td>
        <td><%= link_to 'Destroy', iic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iic', new_iic_path %>
